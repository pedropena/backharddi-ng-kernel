msgstr "Ethernet eller FireWire (IEEE 1394)"

#. Type: text
#. Description
#: ../netcfg-common.templates:171
msgid "Token Ring"
msgstr "Token Ring"

#. Type: text
#. Description
#: ../netcfg-common.templates:175
msgid "USB net"
msgstr "USB-net"

#. Type: text
#. Description
#: ../netcfg-common.templates:184
msgid "Serial-line IP"
msgstr "Seriel-linje IP"

#. Type: text
#. Description
#: ../netcfg-common.templates:188
msgid "Parallel-port IP"
msgstr "Parallelport IP"


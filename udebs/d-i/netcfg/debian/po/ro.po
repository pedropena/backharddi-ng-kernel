msgstr "fără fir (radio)"

#. Type: text
#. Description
#: ../netcfg-common.templates:167
msgid "Ethernet or FireWire (IEEE 1394)"
msgstr "Ethernet sau FireWire (IEEE 1394)"

#. Type: text
#. Description
#: ../netcfg-common.templates:171
msgid "Token Ring"
msgstr "Token Ring"

#. Type: text
#. Description
#: ../netcfg-common.templates:175
msgid "USB net"
msgstr "Reţea USB"

#. Type: text
#. Description
#: ../netcfg-common.templates:184
msgid "Serial-line IP"
msgstr "IP prin linie serială"

#. Type: text
#. Description
#: ../netcfg-common.templates:188
msgid "Parallel-port IP"
msgstr "IP prin port paralel"


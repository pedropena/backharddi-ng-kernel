msgstr "Parametreyên veavakirina toreya vêga:"

#. Type: boolean
#. Description
#: ../netcfg-static.templates:53
msgid ""
" interface     = ${interface}\n"
" ipaddress     = ${ipaddress}\n"
" netmask       = ${netmask}\n"
" gateway       = ${gateway}\n"
" pointopoint   = ${pointopoint}\n"
" nameservers   = ${nameservers}"
msgstr ""
" interface     = ${interface}\n"
" ipaddress     = ${ipaddress}\n"
" netmask       = ${netmask}\n"
" gateway       = ${gateway}\n"
" pointopoint   = ${pointopoint}\n"
" nameservers   = ${nameservers}"


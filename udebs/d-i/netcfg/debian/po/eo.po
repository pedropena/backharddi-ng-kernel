msgid "wireless"
msgstr "sendrata"

#. Type: text
#. Description
#: ../netcfg-common.templates:167
msgid "Ethernet or FireWire (IEEE 1394)"
msgstr "Eterreto aŭ 'FireWire' (IEEE 1394)"

#. Type: text
#. Description
#: ../netcfg-common.templates:171
msgid "Token Ring"
msgstr "Staferingo"

#. Type: text
#. Description
#: ../netcfg-common.templates:175
msgid "USB net"
msgstr "Universala Seria Busreto"

#. Type: text
#. Description
#: ../netcfg-common.templates:184
msgid "Serial-line IP"
msgstr "Interreta Protokolo tra Seria-lino"

#. Type: text
#. Description
#: ../netcfg-common.templates:188
msgid "Parallel-port IP"
msgstr "Interreta Protokolo tra Paraleta-pordo"


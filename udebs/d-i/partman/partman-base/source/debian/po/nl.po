msgstr "IDE%s master (%s)"

#. Type: text
#. Description
#. For example IDE1 slave (hdd)
#: ../partman-base.templates:260
#, no-c-format
msgid "IDE%s slave (%s)"
msgstr "IDE%s slave (%s)"

#. Type: text
#. Description
#. For example IDE1 master, partition #5 (hdc5)
#: ../partman-base.templates:265
#, no-c-format
msgid "IDE%s master, partition #%s (%s)"
msgstr "IDE%s master, partitie #%s (%s)"

#. Type: text
#. Description
#. For example IDE2 slave, partition #5 (hdf5)
#: ../partman-base.templates:270
#, no-c-format
msgid "IDE%s slave, partition #%s (%s)"
msgstr "IDE%s slave, partitie #%s (%s)"

#. Type: text
#. Description
#: ../partman-base.templates:274
#, no-c-format
msgid "SCSI%s (%s,%s,%s) (%s)"
msgstr "SCSI%s (%s,%s,%s) (%s)"

#. Type: text
#. Description
#: ../partman-base.templates:278
#, no-c-format
msgid "SCSI%s (%s,%s,%s), partition #%s (%s)"
msgstr "SCSI%s (%s,%s,%s), partitie #%s (%s)"

#. Type: text
#. Description
#: ../partman-base.templates:282
#, no-c-format
msgid "RAID%s device #%s"
msgstr "RAID%s apparaat #%s"

#. Type: text
#. Description
#: ../partman-base.templates:286
#, no-c-format
msgid "Encrypted volume (%s)"
msgstr "Versleuteld volume (%s)"

#. Type: text
#. Description
#: ../partman-base.templates:290
#, no-c-format
msgid "LVM VG %s, LV %s"
msgstr "LVM VG %s, LV %s"

#. Type: text
#. Description
#: ../partman-base.templates:294
#, no-c-format
msgid "Loopback (loop%s)"
msgstr "Lus-apparaat (loop%s)"

#. Type: text
#. Description
#: ../partman-base.templates:298
#, no-c-format
msgid "DASD %s (%s)"
msgstr "DASD %s (%s)"

#. Type: text
#. Description
#: ../partman-base.templates:302
#, no-c-format
msgid "DASD %s (%s), partition #%s"
msgstr "DASD %s (%s), partitie #%s"

#. Type: text
#. Description
#: ../partman-base.templates:306
msgid "Cancel this menu"
msgstr "Dit menu annuleren"

#. Type: text

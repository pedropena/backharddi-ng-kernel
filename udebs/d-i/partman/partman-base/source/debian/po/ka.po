#. Type: text
#. Description
#: ../partman-base.templates:180
msgid "Please wait..."
msgstr "მოითმინეთ..."

#. Type: text
#. Description
#: ../partman-base.templates:184
msgid "Partitions formatting"
msgstr "დანაყოფების დაფორმატება"

#. Type: text
#. Description
#: ../partman-base.templates:188
msgid "Processing..."
msgstr "დამუშავება..."

#. Type: text
#. Description
#: ../partman-base.templates:200
msgid "Done setting up the partition"
msgstr "დანაყოფის გამართვა დასრულებულია"

#. Type: text
#. Description
#: ../partman-base.templates:204
msgid "Finish partitioning and write changes to disk"
msgstr "დაყოფის დასრულება და ცვლილებების დამტკიცება"

#. Type: text
#. Description
#: ../partman-base.templates:208
msgid "Undo changes to partitions"
msgstr "დანაყოფებში შეტანილი ცვლილებების გაუქმება"

#. Type: text
#. Description
#: ../partman-base.templates:212
msgid "Show Cylinder/Head/Sector information"
msgstr "დისკის გეომეტრია (ცილინდრი/თავაკი/სექტორი)"

#. Type: text
#. Description
#: ../partman-base.templates:216
#, no-c-format
msgid "Dump partition info in %s"
msgstr "დანაყოფის შესახებ ინფორმაციის შენახვა %s-ში"

#. Type: text
#. Description
#. Keep short
#: ../partman-base.templates:221
msgid "FREE SPACE"
msgstr "თავისუფალი ადგილი"

#. Type: text
#. Description
#. "unusable free space".  No more than 8 symbols.
#: ../partman-base.templates:226
msgid "unusable"
msgstr "გამოუსადეგარი"

#. Type: text
#. Description
#. "primary partition".  No more than 8 symbols.
#: ../partman-base.templates:231
msgid "primary"
msgstr "პირველადი"

#. Type: text
#. Description
#. "logical partition".  No more than 8 symbols.
#: ../partman-base.templates:236
msgid "logical"
msgstr "ლოგიკური"

#. Type: text
#. Description
#. "primary or logical".  No more than 8 symbols.
#: ../partman-base.templates:241
msgid "pri/log"
msgstr "პირვ/ლოგ"

#. Type: text
#. Description
#. How to print the partition numbers in your language
#. Examples:
#. %s.
#. No %s
#. N. %s
#: ../partman-base.templates:250
#, no-c-format
msgid "#%s"
msgstr "#%s"

#. Type: text
#. Description
#. For example IDE0 master (hda)
#: ../partman-base.templates:255
#, no-c-format
msgid "IDE%s master (%s)"
msgstr "წამყვანი IDE%s (%s)"

#. Type: text
#. Description
#. For example IDE1 slave (hdd)
#: ../partman-base.templates:260
#, no-c-format
msgid "IDE%s slave (%s)"
msgstr "დამყოლი IDE%s (%s)"

#. Type: text
#. Description
#. For example IDE1 master, partition #5 (hdc5)
#: ../partman-base.templates:265
#, no-c-format
msgid "IDE%s master, partition #%s (%s)"
msgstr "წამყვანი IDE%s, დანაყოფი #%s (%s)"

#. Type: text
#. Description
#. For example IDE2 slave, partition #5 (hdf5)
#: ../partman-base.templates:270
#, no-c-format
msgid "IDE%s slave, partition #%s (%s)"
msgstr "დამყოლი IDE%s, დანაყოფი #%s (%s)"

#. Type: text
#. Description
#: ../partman-base.templates:274
#, no-c-format
msgid "SCSI%s (%s,%s,%s) (%s)"
msgstr "SCSI%s (%s,%s,%s) (%s)"

#. Type: text
#. Description
#: ../partman-base.templates:278
#, no-c-format
msgid "SCSI%s (%s,%s,%s), partition #%s (%s)"
msgstr "SCSI%s (%s,%s,%s), დანაყოფი #%s (%s)"

#. Type: text
#. Description
#: ../partman-base.templates:282
#, no-c-format
msgid "RAID%s device #%s"
msgstr "RAID%s მოწყობილობა #%s"

#. Type: text
#. Description
#: ../partman-base.templates:286
#, no-c-format
msgid "Encrypted volume (%s)"
msgstr "დაშიფრული დისკწამყვანი (%s)"

#. Type: text
#. Description
#: ../partman-base.templates:290
#, no-c-format
msgid "LVM VG %s, LV %s"
msgstr "LVM VG %s, LV %s"

#. Type: text
#. Description
#: ../partman-base.templates:294
#, no-c-format
msgid "Loopback (loop%s)"
msgstr "Loopback-მოწყობილობა (loop%s)"

#. Type: text
#. Description
#: ../partman-base.templates:298
#, no-c-format
msgid "DASD %s (%s)"
msgstr "DASD %s (%s)"

#. Type: text
#. Description
#: ../partman-base.templates:302
#, no-c-format
msgid "DASD %s (%s), partition #%s"
msgstr "DASD %s (%s), დანაყოფი #%s"

#. Type: text
#. Description
#: ../partman-base.templates:306
msgid "Cancel this menu"
msgstr "ამ მენიუს გაუქმება"

#. Type: text
#. Description
#. Main menu entry
#: ../partman-base.templates:311
msgid "Partition disks"
msgstr "დისკების დაყოფა"

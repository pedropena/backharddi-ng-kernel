#!/bin/sh

dev=$1
id=$2

cd $dev

if [ -f $id/gen ]; then
	[ "$(cat $id/detected_filesystem)" = "linux-swap" ] && exit 0
	if [ -f $id/compresion ]; then 
		case "$(cat $id/compresion)" in
			lzo) printf "compresion\tCon compresi칩n LZO (r치pida)\n" ;;
			lzma) printf "compresion\tCon compresi칩n LZMA (muy lenta)\n" ;;
		esac	
	else
		printf "compresion\tSin compresi칩n\n"
	fi
fi
